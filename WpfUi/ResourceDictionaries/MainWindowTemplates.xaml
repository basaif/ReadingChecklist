<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:Components="clr-namespace:WpfUi.Views.Components"
                    xmlns:Models="clr-namespace:WpfUi.Models"
                    xmlns:ViewModels="clr-namespace:WpfUi.ViewModels"
                    xmlns:Views="clr-namespace:WpfUi.Views">
    <DataTemplate DataType="{x:Type ViewModels:HomeViewModel}">
        <Views:HomeView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type ViewModels:GetBooksViewModel}">
        <Views:GetBooksView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type ViewModels:BookCardViewModel}">
        <Views:BookCardView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type ViewModels:TagListViewModel}">
        <Views:TagListView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type ViewModels:BookListViewModel}">
        <Views:BookListView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type Models:SelectableTagModel}">
        <Components:SelectableTagUc />
    </DataTemplate>
</ResourceDictionary>