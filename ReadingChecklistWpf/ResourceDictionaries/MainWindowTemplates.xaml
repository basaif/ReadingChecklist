<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:Views="clr-namespace:ReadingChecklistWpf.Views"
                    xmlns:ViewModels="clr-namespace:ReadingChecklistWpf.ViewModels">
    <DataTemplate DataType="{x:Type ViewModels:HomeViewModel}">
        <Views:HomeView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type ViewModels:NoBooksViewModel}">
        <Views:NoBooksView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type ViewModels:BookCardViewModel}">
        <Views:BookCardView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type ViewModels:RefreshBooksViewModel}">
        <Views:RefreshBooksView/>
    </DataTemplate>
</ResourceDictionary>